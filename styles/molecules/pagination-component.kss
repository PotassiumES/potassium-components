/* PaginationComponent */

.dom.pagination-component > .left-arrow,
.dom.pagination-component > .right-arrow {
	width: 33px;
	display: inline-block;
	vertical-align: top;
	cursor: pointer;
}

.dom.pagination-component > .left-arrow.disabled,
.dom.pagination-component > .right-arrow.disabled {
	opacity: 0.1;
}


.dom.pagination-component > .label-component {
	text-align: center;
	min-width: 2em;
	margin: 0 5px;
}
