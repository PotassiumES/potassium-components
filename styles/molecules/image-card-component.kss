
/*** ImageCardComponent ***/

.dom.image-card-component > .dom.main-component > .dom.image-component {
	max-width: 100%;
	background-color: var(--dom-background-color);
}

.dom.image-card-component > .main-component > :first-child {
	margin-left: auto;
	margin-right: auto;
	display: block;
}

.som.image-card-component > .main-component > .som.image-component {
	centroid-y: 10cm;
	scale: 0.5 0.5 0.05;
}