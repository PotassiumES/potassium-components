/* AudioPlayerComponent */

div.dom.audio-player-component {
	display: grid;
	grid-template: 44px 30px / 60px;
	gap: var(--inner-element-padding);
}

.dom.audio-player-component > .audio-component {
	width: 60px;
}

.som.audio-player-component > .audio-component {
	centroid-y: 10cm;
}

.som.audio-player-component > .toggle-component {
	centroid: -11cm -10cm 0;
}
